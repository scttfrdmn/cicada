# AWS Integration Test Configuration
# Copy this file to .env.test and fill in your AWS credentials
# DO NOT commit .env.test - it contains sensitive credentials!

# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-west-2
AWS_PROFILE=cicada-test

# Test S3 Bucket (will be created if it doesn't exist)
# Use a unique name to avoid conflicts
CICADA_TEST_BUCKET=cicada-integration-tests-${USER}

# Test Resource Configuration
CICADA_TEST_PREFIX=test-$(date +%s)
CICADA_CLEANUP_AFTER_TESTS=true

# Safety Limits
CICADA_MAX_TEST_OBJECTS=1000
CICADA_MAX_TEST_SIZE_GB=10
